{
  "name": "@parrotpay/sdk",
  "version": "0.1.0",
  "description": "Add Tempo payments to any site in one line",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": ["dist", "README.md"],
  "scripts": {
    "build": "pnpm exec tsup src/index.tsx --format cjs,esm --dts --external react --external react-dom",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --watch --external react --external react-dom",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parrotpay/tempo.git"
  },
  "keywords": ["tempo", "payments", "crypto", "blockchain", "checkout", "wallet"],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {}
}
